# OpenRouter API Configuration (for most models)
OPENROUTER_API_KEY=sk-or-v1-your-api-key-here

# OpenAI API Configuration (for o4-mini model - not supported by OpenRouter)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Model Configuration
GENERATION_MODEL=anthropic/claude-sonnet-4
JUDGE_MODEL=anthropic/claude-sonnet-4
QWEN_MODEL=qwen/qwen3-max

# Cross-check models (comma-separated, use actual OpenRouter model IDs)
CROSSCHECK_MODELS=deepseek/deepseek-chat-v3.1,google/gemini-2.5-flash-preview,x-ai/grok-beta,meta-llama/llama-3.3-70b-instruct

# Generation Settings
TARGET_COUNT=50
MAX_CONCURRENT_REQUESTS=10
REQUESTS_PER_MINUTE=60

# Validation Thresholds
QWEN_MAX_PASS_RATE=0.05
MIN_CROSSCHECK_MODELS=2
MIN_CROSSCHECK_TOTAL=5
CORRECTNESS_THRESHOLD=0.90

# Output
OUTPUT_DIR=output
